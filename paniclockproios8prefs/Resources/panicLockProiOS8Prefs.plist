<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>

		<dict>
			<key>cell</key> 		<string>PSGroupCell</string>
			<key>label</key> 		<string>Configure</string>
		</dict>

		<dict>
		    <key>cell</key> 			<string>PSLinkCell</string>
		    <key>label</key> 			<string>Activation Methods</string>
		    <key>isController</key> 	<true/>
		    <key>bundle</key> 			<string>LibActivator</string>
		    <key>activatorListener</key> <string>com.ruslan.paniclockproios8</string>
		</dict>

		<dict>
			<key>cell</key>  			<string>PSLinkCell</string>
			<key>bundle</key> 			<string>AppList</string>
			<key>isController</key> 	<string>1</string>
			<key>label</key> 			<string>Protected Applications</string>
			<key>ALAllowsSelection</key> 		<string>1</string>
			<key>ALChangeNotification</key>  	<string>com.ruslan.paniclockproios8prefs/settingschanged</string>

			<key>ALSectionDescriptors</key>
			<array>
				<dict>
					<key>footer-title</key>  	<string>Tap on applications to protect.</string>
					<key>items</key>
					<array/>
				</dict>
				<dict>
					<key>cell-class-name</key>
					<string>ALCheckCell</string>
					<key>icon-size</key>
					<string>29</string>
					<key>suppress-hidden-apps</key>
					<string>1</string>
					<key>title</key>
					<string>All Applications</string>
				</dict>
			</array>
			<key>ALSettingsKeyPrefix</key>
			<string>App-</string>
			<key>ALSettingsPath</key>
			<string>/var/mobile/Library/Preferences/com.ruslan.panicLockProiOS8Prefs.plist</string>
		</dict>

		<dict>
			<key>action</key> 			<string>sendEmail</string>
			<key>cell</key> 			<string>PSButtonCell</string>
			<key>label</key> 			<string>Issue? Let me know!</string>
			<key>icon</key> 			<string>mail.png</string>
		</dict>

		<dict>
			<key>cell</key> 		<string>PSGroupCell</string>
			<key>label</key> 		<string> </string>
			<key>footerText</key> 	<string>www.ruslanArdashev.com</string>
		</dict>

		<dict>
			<key>action</key> 			<string>openDemo</string>
			<key>cell</key> 			<string>PSButtonCell</string>
			<key>label</key> 			<string>panicLock iOS 8 Demo</string>
			<key>icon</key>  			<string>youtube.png</string>
		</dict>

		<dict>
			<key>action</key> 			<string>openInstructions</string>
			<key>cell</key> 			<string>PSButtonCell</string>
			<key>label</key> 			<string>How to use panicLock for iOS8</string>
			<key>icon</key>  			<string>youtube.png</string>
		</dict>

		<dict>
			<key>action</key> 			<string>openYoutubeChannel</string>
			<key>cell</key> 			<string>PSButtonCell</string>
			<key>label</key> 			<string>Personal Youtube Channel</string>
			<key>icon</key>  			<string>youtube.png</string>
		</dict>

	</array>
	<key>title</key>
	<string>panicLock [Pro] iOS8</string>
</dict>
</plist>
